    "@fortawesome/free-solid-svg-icons": "^5.15.1",
    "@fortawesome/react-fontawesome": "^0.1.14",
    "axios": "^0.18.0",
    "bootstrap": "^4.5.3",
    "express": "^4.16.3",
    "firebase": "^8.2.1",
    "if-env": "^1.0.4",
    "mongoose": "^5.11.8",
